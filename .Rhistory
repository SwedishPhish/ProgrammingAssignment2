pollutantmean <- function(directory,pollutant,id = 1:332) {
files <- list.files(directory,pattern = "*.csv",full.names=TRUE)
#     names.list <- lapply(files,function(x) read.csv(x,header=TRUE))
temp <- c()
for (i in id) {
data <- read.csv(files[i],header=TRUE)
no_na <- data[!is.na(data[,pollutant]), pollutant]
temp <- c(temp, no_na)
}
mean(temp)
}
